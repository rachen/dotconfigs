{
    "sublimeTextKeymap.promptV3Features": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.snippetSuggestions": "top",
    "editor.formatOnPaste": true,
    "python.disablePromptForFeatures": [
        "pylint"
    ],
    "window.zoomLevel": 0,
    "editor.fontSize": 14,
    "editor.rulers": [
        80
    ],
    "workbench.colorTheme": "Ayu One Dark",
    "gitlens.advanced.messages": {
        "suppressCommitHasNoPreviousCommitWarning": false,
        "suppressCommitNotFoundWarning": false,
        "suppressFileNotUnderSourceControlWarning": false,
        "suppressGitVersionWarning": false,
        "suppressLineUncommittedWarning": false,
        "suppressNoRepositoryWarning": false,
        "suppressUpdateNotice": true,
        "suppressWelcomeNotice": true
    },
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/*.pyc": true
    },
    "python.linting.enabled": false,
    "python.linting.pycodestyleEnabled": true,
    "editor.acceptSuggestionOnEnter": "on",
    "editor.quickSuggestionsDelay": 50,
    "editor.find.autoFindInSelection": false,
    "editor.autoIndent": true,
    "window.closeWhenEmpty": false,
    "editor.emptySelectionClipboard": true,
    "files.encoding": "utf8",
    "editor.detectIndentation": true,
    "editor.dragAndDrop": true,
    "editor.renderIndentGuides": true,
    "editor.renderWhitespace": "boundary",
    "files.insertFinalNewline": false,
    "editor.showFoldingControls": "mouseover",
    "editor.find.seedSearchStringFromSelection": true,
    "editor.fontFamily": "Menlo",
    "files.hotExit": "onExit",
    "editor.wrappingIndent": "same",
    "editor.lineNumbers": "on",
    "editor.matchBrackets": true,
    "editor.selectionHighlight": true,
    "window.openFilesInNewWindow": "on",
    "window.restoreFullscreen": false,
    "files.autoSave": "onFocusChange",
    "editor.scrollBeyondLastLine": true,
    "editor.mouseWheelScrollSensitivity": 1,
    "window.title": "${activeEditorFull}${separator}${rootName}",
    "workbench.editor.tabCloseButton": "right",
    "editor.tabCompletion": true,
    "editor.tabSize": 4,
    "editor.trimAutoWhitespace": true,
    "files.trimTrailingWhitespace": true,
    "editor.useTabStops": false,
    "editor.wordSeparators": "./\\()\"'-:,.;<>~!@#$%^&*|+=[]{}`~?",
    "editor.wordWrap": "on",
    "workbench.colorCustomizations": {
        "editor.selectionBackground": "#e74c3c",
        "editor.selectionForeground": "#fff",
        "editor.selectionHighlightBorder": "#fff",
        // "statusBar.background": "#333333",
        // "statusBar.foreground": "#8c8c8c"
        "editorLineNumber.foreground": "#4f4f4f",
        "editorRuler.foreground": "#20272c"
    },
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "editor.wordWrapColumn": 100,
    "editor.cursorBlinking": "smooth",
    "workbench.statusBar.feedback.visible": false,
    "breadcrumbs.enabled": true,
    "python.pythonPath": "/usr/local/bin/python",
    "vsicons.dontShowNewVersionMessage": true,
    "workbench.iconTheme": "vscode-icons"
}